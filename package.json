{
  "name": "momentum-design-kit",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momentum-design/momentum-design-kit.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap && lerna link",
    "bootstrap:clean": "run-s clean bootstrap",
    "clean": "rimraf node_modules/ **/node_modules/",
